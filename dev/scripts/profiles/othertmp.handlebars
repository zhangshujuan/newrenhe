{{#is type "===" "contact"}}
	{{#is data.email "not" ""}}
	<dl class="renhe-dl">
		<dt>{{map i18n.label 14}}</dt>
		<dd>{{data.email}}</dd>
	</dl>
	{{/is}}
	{{#is data.mobile "not" ""}}
	<dl class="renhe-dl">
		<dt>{{map i18n.label 15}}</dt>
		<dd data-bind-contact="mobile">{{data.mobile}}</dd>
	</dl>
	{{/is}}
	{{#is data.weixin "not" ""}}
