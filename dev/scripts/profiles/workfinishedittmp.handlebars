	<div class="text-right js-add-opts">
		<p><a href="javascript:void(0);" class="js-add-panel" data-template="#WorkExperienceTemplate">{{map i18n/text 8}}{{map i18n/text 9}}</a></p>
	</div>
	{{#each data.experienceVoList}}
	<form class="form-horizontal">
		<div class="form-group">
			<label class="col-sm-1 control-label">{{map ../i18n/label 0}}</label>
		    <div class="col-sm-4">
		    	<input type="hidden" name="id" value="{{id}}">
		      <input type="text" class="form-control" name="title" value="{{experience.title}}" validate="{validate:{required:true},messages:{required:'{{map ../i18n/massages 0}}{{map ../i18n/massages 2}}'}}">
		    </div>
		</div>
		<div class="form-group">
			<label class="col-sm-1 control-label">{{map ../i18n/label 1}}</label>
		    <div class="col-sm-4">
		      <input type="text" class="form-control" name="company" value="{{experience.company}}" validate="{validate:{required:true},messages:{required:'{{map ../i18n/massages 0}}{{map ../i18n/massages 3}}'}}">
		    </div>
		</div>
		<div class="form-group">
			<label class="col-sm-1 control-label">{{map ../i18n/label 2}}</label>
		    <div class="col-sm-11 sxtime">
		    	<input type="text" class="form-control" name="startYear" value="{{startYear}}" validate="{validate:{required:true,digits:true,cnowYear:true},messages:{required:'{{map ../i18n/massages 0}}{{map ../i18n/massages 4}}',digits:'{{map ../i18n/massages 0}}{{map ../i18n/massages 5}}',cnowYear:'{{map ../massages 6}}'}}">
		      	<select name="startMonth" class="form-control" value="{{startDate}}" validate="{validate:{required:true,cnowMonth:'[name=startYear]'},messages:{required:'{{map ../i18n/massages 1}}{{map ../i18n/massages 7}}',cnowMonth:'{{map ../i18n/massages 8}}'}}">
					<option value="">{{map ../i18n/text 1}}</option>
					{{#each ../i18n/monthList}}
    				<option value="{{this}}" {{#is ../startMonth "===" this}}selected{{/is}}>{{this}}</option>
					{{/each}}
				</select>
				<span class="form-control-static">{{map ../i18n/text 2}}</span>
				<input type="text" class="form-control" name="endYear" {{#is experience.status "===" 1}}disabled value=""{{else}}value="{{endYear}}"{{/is}} validate="{validate:{required:true,digits:true,cnowYear:true},messages:{required:'{{map ../i18n/massages 0}}{{map ../i18n/massages 9}}',digits:'{{map ../i18n/massages 0}}{{map ../i18n/massages 10}}',cnowYear:'{{map ../i18n/massages 11}}'}}">
		      	<select name="endMonth"  class="form-control" {{#is experience.status "===" 1}}disabled value=""{{else}}value="{{endMonth}}"{{/is}} validate="{validate:{required:true,cnowMonth:'[name=endYear]',compareDate:true},messages:{required:'{{map ../i18n/massages 1}}{{map ../i18n/massages 12}}',cnowMonth:'{{map ../i18n/massages 13}}',compareDate:'{{map ../i18n/massages 14}}'}}">
					<option value="">{{map ../i18n/text 1}}</option>
					{{#each ../i18n/monthList}}
    				<option value="{{this}}" {{#is ../endMonth "===" this}}selected{{/is}}>{{this}}</option>
					{{/each}}
				</select>
				<div class="checkbox inline-block">
				  <label>
				    <input type="checkbox" value="1" {{#is experience.status "===" 1}}checked{{/is}} name="now">{{map ../i18n/text 3}}
				  </label>
				</div>
		    </div>
		</div>
		<div class="form-group">
			<label class="col-sm-1 control-label">{{map ../i18n/label 3}}</label>
		    <div class="col-sm-11 industry-select js-area-industry">
		      	<select class="form-control primary" name="industry_Parents" data-value="{{parentIndustryId}}" value="{{parentIndustryId}}" validate="{validate:{required:true},messages:{required:'{{map ../i18n/massages 1}}{{map ../i18n/massages 15}}'}}"></select> 
				<select class="form-control secondary" name="industry" data-value="{{experience.industry}}" value="{{experience.industry}}" validate="{validate:{required:true},messages:{required:'{{map ../i18n/massages 1}}{{map ../i18n/massages 15}}'}}"></select> 
		    </div>
		</div>
		<div class="form-group">
			<label class="col-sm-1 control-label">{{map ../i18n/label 4}}</label>
		    <div class="col-sm-11 industry-select">

		      	<select class="form-control primary" name="orgtype" value="{{experience.orgSize}}" validate="{validate:{required:true},messages:{required:'{{map ../i18n/massages 1}}{{map ../i18n/massages 16}}'}}">
					{{#each ../i18n/typeList}}
    				<option value="{{@index}}" {{#is ../experience.orgSize "===" @index}}selected{{/is}}>{{this}}</option>
					{{/each}}
				</select> 
		    </div>
		</div>
		
		<div class="form-group">
			<label class="col-sm-1 control-label">{{map ../i18n/label 5}}</label>
		    <div class="col-sm-11 industry-select">
				
		      	<select class="form-control primary" name="orgsize" value="{{experience.orgSizeName}}" validate="{validate:{required:true},messages:{required:'{{map ../i18n/massages 1}}{{map ../i18n/massages 17}}'}}">
		
					{{#each ../i18n/sizeList}}
    				<option value="{{@index}}" {{#is ../experience.orgSize "===" @index}}selected{{/is}}>{{this}}</option>
					{{/each}}
				</select> 
		    </div>
		</div>
		<div class="form-group">
			<label class="col-sm-1 control-label">{{map ../i18n/label 6}}</label>
		    <div class="col-sm-11 industry-select">
		      	<input type="text" class="form-control" name="website" value="{{experience.website}}">
		    </div>
		</div>
		<div class="form-group">
		
			<label class="col-sm-1 control-label">{{map ../i18n/label 7}}</label>
		    <div class="col-sm-11">
		      	<textarea class="form-control js-textlimit" data-limit="600" name="positionDesc" rows="3" validate="{validate:{required:true,maxlength:600},messages:{required:'{{map ../i18n/massages 0}}{{map ../i18n/massages 18}}',maxlength:'{{map ../i18n/massages 18}}{{map ../i18n/massages 19}}'}}">{{experience.positionDesc}}</textarea>
		      	<p class="form-control-static">{{map ../i18n/text 5}} <span class="red js-num">600</span> {{map ../i18n/text 6}}</p>
		    </div>
		</div>
		<div class="text-right">
			<p><a href="javascript:void(0);" class="js-remove-panel">{{map ../i18n/text 7}}{{map ../i18n/text 9}}</a></p>
		</div>
	</form>
	{{/each}}